{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": false,
    "rootDir": "src",
    "outDir": "dist"
  },
  
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "**/*.spec.ts",
	"node_modules"
  ]
}